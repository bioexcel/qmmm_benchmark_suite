&GLOBAL
  PRINT_LEVEL LOW
  PROJECT GROMACS
  RUN_TYPE MD
&END GLOBAL
&FORCE_EVAL
  METHOD QMMM
  &DFT
    CHARGE -1
    MULTIPLICITY 1
    BASIS_SET_FILE_NAME  BASIS_MOLOPT
    POTENTIAL_FILE_NAME  POTENTIAL
    &MGRID
      NGRIDS 5
      CUTOFF 400
      REL_CUTOFF 50
      COMMENSURATE
    &END MGRID
    &SCF
      SCF_GUESS RESTART
      EPS_SCF 5.0E-7
      MAX_SCF 20
      &OT  T
        MINIMIZER  DIIS
        STEPSIZE   0.15
        PRECONDITIONER FULL_ALL
      &END OT
      &OUTER_SCF  T
        MAX_SCF 20
        EPS_SCF 5.0E-7
      &END OUTER_SCF
    &END SCF
    &XC
      DENSITY_CUTOFF     1.0E-10
      GRADIENT_CUTOFF     1.0E-10
      TAU_CUTOFF     1.0E-10
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
    &QS
     METHOD GPW
     EPS_DEFAULT 1.0E-10
     EXTRAPOLATION ASPC
     EXTRAPOLATION_ORDER  4
    &END QS
  &END DFT
  &QMMM
    &CELL
         ABC 16.065 16.065 16.065
      PERIODIC XYZ
    &END CELL
    ECOUPL GAUSS
    USE_GEEP_LIB 15
    &PERIODIC
      &MULTIPOLE ON
         RCUT     1.0000000000000007E+01
         EWALD_PRECISION     9.9999999999999995E-07
      &END
    &END PERIODIC
    &QM_KIND H  
      MM_INDEX 2 3 4 6 7 8 16 19 21 23 25
    &END QM_KIND
    &QM_KIND C  
      MM_INDEX 1 5 9 12 14 15 17 18 20 22 24 26
    &END QM_KIND
    &QM_KIND N  
      MM_INDEX 10 11
    &END QM_KIND
    &QM_KIND O  
      MM_INDEX 13 27
    &END QM_KIND
  &END QMMM
  &MM
    &FORCEFIELD
      PARMTYPE AMBER
      DO_NONBONDED .TRUE.
      PARM_FILE_NAME gfp-new.prmtop
      EI_SCALE14 1.0
      VDW_SCALE14 1.0
      &SPLINE
      !  EMAX_SPLINE 1.0E14
      !  RCUT_NB [angstrom] 12
      &END SPLINE
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE SPME
        ALPHA .40
        GMAX 80
      &END EWALD
    &END POISSON
    &PRINT
#      &FF_INFO OFF
#      &END FF_INFO
    &END PRINT
 &END MM
  &SUBSYS
    &CELL
      A 51.145 0.000 0.000
      B 0.000 46.110 0.000
      C 0.000 0.000 42.404
      PERIODIC XYZ
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME gfp-new.pdb
      COORD_FILE_FORMAT PDB
      CONN_FILE_FORMAT AMBER
      CONN_FILE_NAME gfp-new.prmtop
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
    &KIND H  
      ELEMENT H  
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C  
      ELEMENT C  
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND N  
      ELEMENT N  
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE-q5
    &END KIND
    &KIND O  
      ELEMENT O  
      BASIS_SET DZVP-MOLOPT-GTH
      POTENTIAL GTH-PBE-q6
    &END KIND
    &KIND NA+
      ELEMENT NA
    &END KIND
    &PRINT
       &CELL
          ADD_LAST NUMERIC
          FILENAME CELL
       &END CELL
    &END PRINT
  &END SUBSYS
  &PRINT
    &FORCES ON
    &END FORCES
  &END PRINT
&END FORCE_EVAL
&MOTION
   &MD
      ENSEMBLE NVE
      STEPS 500
      TEMPERATURE 300
      TIMESTEP 0.5
   &END MD
&END MOTION
